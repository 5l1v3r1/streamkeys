{
  "name": "Grooveshark Hotkeys",
  "description": "",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "http://grooveshark.com/*"
      ],
      "js": [
        "GroovesharkProxy.js",
        "content.js"
      ],
      "run_at": "document_idle",
      "patterns": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Grooveshark Proxy Extension"
  }
}